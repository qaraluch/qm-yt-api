{
  "name": "XXX",
  "homepage": "https://github.com/qaraluch/XXX",
  "version": "0.0.1",
  "description": "YYY",
  "license": "MIT",
  "author": {
    "name": "qaraluch",
    "email": "prezenciq@gmail.com",
    "url": "https://github.com/qaraluch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qaraluch/XXX.git"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "main": "./dist/index.js",
  "module": "./src/index.js",
  "scripts": {
    "build": "babel src --out-dir=dist",
    "test": "ava --verbose \"test/*.test.js\"",
    "start": "npm run build && node dist/index.js",
    "start:test": "npm run build -s && npm run test -s",
    "prepublishOnly": "npm run build",
    "watch": "nodemon --watch src --watch test -x \"npm run start:test -s\"",
    "clear": "rimraf dist/*",
    "clear:all": "rimraf node_modules"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "ava": "0.22.0",
    "babel-register": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "eslint": "4.9.0",
    "eslint-plugin-import": "2.8.0",
    "babel-eslint": "8.0.1",
    "prettier": "1.7.4"
  },
  "dependencies": {
    "babel-runtime": "6.26.0"
  }
}
